{
	"name": "obsidian-project-manager",
	"version": "1.0.0",
	"description": "Comprehensive project and task management plugin for Obsidian with Supabase integration",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "plugin", "project-management", "tasks", "supabase"],
	"author": "Allan Elfe",
	"license": "MIT",
	"dependencies": {
		"@supabase/supabase-js": "^2.39.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/sortable": "^8.0.0",
		"@dnd-kit/utilities": "^3.2.2"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.45",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
